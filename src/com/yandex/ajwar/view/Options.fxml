                                       </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                    <VBox alignment="TOP_CENTER" prefHeight="85.0" spacing="3.0">
                                       <children>
                                          <TextField fx:id="textFieldFormats" maxWidth="106.0" />
